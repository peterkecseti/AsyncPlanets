(() => { "use strict"; document.addEventListener("DOMContentLoaded", (async () => { document.getElementById("btn1").addEventListener("click", (async () => { console.log("asd"); let e = await fetch("planets.json"); console.log(e); let t = await e.json(); t.planets.sort(((e, t) => e.name.localeCompare(t.name))); let n = document.getElementById("osszesBolygo"); n.textContent = ""; for (let e of t.planets) { let t = document.createElement("li"); n.appendChild(t), e.dwarf ? t.innerHTML = "<i>" + e.name : t.innerHTML = e.name } })), document.getElementById("btn2").addEventListener("click", (async () => { let e = await fetch("planets.json"); console.log(e); let t = await e.json(), n = "", a = []; for (let e of t.planets) { let t = Math.sqrt(e.area / 4 * Math.PI); a.push(t) } for (let e of a) n += e + "; "; document.getElementById("diameters").innerHTML = n })), document.getElementById("btn3").addEventListener("click", (async () => { let e = await fetch("planets.json"); console.log(e), await e.json(); let t = document.getElementById("min").value, n = document.getElementById("max").value, a = []; a.push(planets.name.filter((e => e.area >= t && e.area <= n))), console.log(a) })) })) })();